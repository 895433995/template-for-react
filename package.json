{
  "name": "template-for-react",
  "displayName": "template-for-react",
  "description": "create template for react",
  "version": "1.0.0",
  "publisher": "tchen",
  "repository": "https://github.com/895433995/template-for-react",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Extension Packs",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:template-for-react.createReactClassComponent"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "template-for-react.createReactClassComponent",
        "title": "新建React Class模板代码"
      },
      {
        "command": "template-for-react.createReactFunctionComponent",
        "title": "新建React Funtion模板代码"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "group": "1_modification",
          "command": "template-for-react.createReactClassComponent"
        },
        {
          "when": "explorerResourceIsFolder",
          "group": "1_modification",
          "command": "template-for-react.createReactFunctionComponent"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.50.0",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "ts-loader": "^8.0.4",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
